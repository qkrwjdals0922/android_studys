    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="check_onclick"
        android:text="check 버튼"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onclick2"
        android:text="공지 버튼"/>
</LinearLayout>